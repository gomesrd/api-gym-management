{
  "name": "api-gym-management",
  "version": "1.0.1",
  "description": "API for gym management system",
  "main": "index.js",
  "author": "Douglas Santos",
  "license": "ISC",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "dotenv -e .env.docker nodemon src/index.ts",
    "migrate-dev": "dotenv -e .env.docker prisma migrate dev --name init",
    "migrate": "prisma migrate deploy",
    "lint": "eslint --fix \"src/**/*.{js,ts}\"",
    "format": "prettier --write \"src/**/*.{js,ts}\""
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/jwt": "^5.0.1",
    "@prisma/client": "^5.8.1",
    "amazon-cognito-identity-js": "^6.3.7",
    "aws-jwt-verify": "^4.0.1",
    "aws-sdk": "^2.1562.0",
    "dotenv": "^16.4.1",
    "dotenv-cli": "^7.3.0",
    "envalid": "^8.0.0",
    "fastify": "^4.25.2",
    "fastify-swagger": "^5.2.0",
    "fastify-zod": "^1.4.0",
    "yarn": "^1.22.21",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "dotenv": "^16.4.1",
    "dotenv-cli": "^7.3.0",
    "envalid": "^8.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "prisma": "^5.8.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
